def multiply(n1,n2):
    if n1==0 or n2==0:
        raise ValueError()
    else:
        return n1*n2
             
def main():
    try:
        num1=int(input("enter first number"))
        num2=int(input("enter second number"))
        num3=multiply(num1,num2)
        print(num1,num2,num3)
    except ValueError:
        print("can not multiply Number with zero")
main()
