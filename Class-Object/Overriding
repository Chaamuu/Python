class A:
    def m1(self):#overriden method
        print("m1 of A")
        
class B(A):
    def m2(self):
        print("m2 of B")
    def m1(self):#overriding metod
        print("overriding method")
        
def main():
    objb=B()
    objb.m1()
    objb.m2()
main()

##
class A:
    def m1(self):
        print("m1 of A")
        
class B(A):
    def m2(self):
        print("m2 of B")
        
objb=B()
objb.m1()
objb.m2()
